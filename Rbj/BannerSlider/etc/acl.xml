<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin" title="Magento Admin">
                <resource id="Rbj_BannerSlider::banners" title="Banner Slider" sortOrder="10">
                    <resource id="Rbj_BannerSlider::manage_banners" title="Manage Banners" sortOrder="10" />
                    <resource id="Rbj_BannerSlider::manage_config" title="Configuration" sortOrder="20" />
                </resource>
            </resource>
        </resources>
    </acl>
</config>
